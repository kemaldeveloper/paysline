mixin title-1(title, accent)
  h1.title= title
    span.accent= accent

mixin title-2(title)
  h2.title-2= title

mixin title-3(title)
  h3.title-3= title

mixin title-4(title)
  h4.title-4= title

mixin title-5(title)
  h5.title-5= title

mixin undersub
  div.undersub
    p 
      block

mixin body
  div.body
    p 
      block

mixin navLink(url, title)
  a.nav-link(href="url")=title

mixin link(url, title)
  a.link(href=url)= title

mixin accentLink(title, url)
  a.accent-link(href=url)= title

mixin button(tag, text, url, size='large',)
  #{tag}.btn(class=`btn--${size}` href=url)= text

mixin input(type, name, placeholder, required)
  .input-wrap
    input.form-control(type=type, name=name, placeholder=placeholder, required=required)

mixin cookieBtn(title, state)
  button.cookie-btn(class=`cookie-btn--${state}` type=button)= title

