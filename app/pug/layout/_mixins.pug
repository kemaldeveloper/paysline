mixin tabBtn(text, id, active=false)
  button.nav-link(class=active ? 'active' : '' id=`nav-${id}-tab` data-bs-toggle="tab" data-bs-target=`#nav-${id}` type="button" role="tab" aria-controls=`nav-${id}` aria-selected=active)= text

mixin tabContent(id, active=false)
  .tab-pane.fade(class=active ? 'show active' : '' id=`nav-${id}` role="tabpanel" aria-labelledby=`nav-${id}-tab` tabindex="0")
    block

mixin checkbox
  div.form-check
    input.form-check-input(type="checkbox"  id="flexCheckDefault" name="checkbox")
    label.form-check-label(for="flexCheckDefault")
      | Remember me